<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Suppress known false positives for AWS SDK -->
    <suppress>
        <notes>AWS SDK components are regularly updated and false positives are common</notes>
        <packageUrl regex="true">^pkg:maven/software\.amazon\.awssdk/.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
        <cvssBelow>7.0</cvssBelow>
    </suppress>
    
    <!-- Suppress test scope dependencies -->
    <suppress>
        <notes>Test dependencies don't affect production</notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\..*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
    <suppress>
        <notes>Test dependencies don't affect production</notes>
        <packageUrl regex="true">^pkg:maven/org\.mockito/.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
    <!-- Suppress low severity issues in Jackson (commonly used, well-maintained) -->
    <suppress>
        <notes>Jackson is actively maintained and low severity issues are acceptable</notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\..*$</packageUrl>
        <cvssBelow>7.0</cvssBelow>
    </suppress>
    
    <!-- Suppress development/build time dependencies -->
    <suppress>
        <notes>CDK dependencies are build-time only</notes>
        <packageUrl regex="true">^pkg:maven/software\.amazon\.awscdk/.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
</suppressions>